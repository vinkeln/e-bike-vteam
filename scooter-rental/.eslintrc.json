{
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "no-unused-vars": "warn",  // Ändra detta till 'warn' eller 'off' beroende på dina behov
      "eqeqeq": "error",
      "no-undef": "error",
      "no-prototype-builtins": "off"  // Stäng av eller hantera specifikt om du föredrar det
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  }
  